        "/predict_ticker/{ticker}": {
            "get": {
                "tags": [
                    "Predictions"
                ],
                "summary": "Predict Stock Movement",
                "description": "Get ML prediction for a specific stock ticker.\n\n    Uses machine learning model to predict likely price movement based on:\n    - Technical indicators (RSI, MACD, Bollinger Bands, Momentum)\n    - Historical price patterns\n    - Volume analysis\n\n    Returns probability (0-1) where higher values indicate higher confidence\n    of upward movement.",
                "operationId": "predict_ticker_predict_ticker__ticker__get",
                "parameters": [
                    {
                        "name": "ticker",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "title": "Ticker"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful Response",
