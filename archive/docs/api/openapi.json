        },
        "/ranking": {
            "get": {
                "tags": [
                    "Predictions"
                ],
                "summary": "Stock Rankings",
                "description": "Get ranked list of stocks based on ML predictions.\n\n    Supports filtering by:\n    - Country/region (e.g., 'Switzerland', 'Germany', 'United States')\n    - Custom ticker list (comma-separated)\n    - Top N results\n\n    Returns stocks sorted by prediction probability (highest first).",
                "operationId": "ranking_ranking_get",
                "parameters": [
                    {
                        "name": "tickers",
                        "in": "query",
                        "required": false,
                        "schema": {
                            "type": "string",
                            "default": "",
                            "title": "Tickers"
                        }
                    },
                    {
                        "name": "country",
                        "in": "query",
                        "required": false,
                        "schema": {
                            "type": "string",
                            "default": "Global",
                            "title": "Country"
                        }
                    }
                ],
                "responses": {
                    "200": {
